<meta charset="UTF-8">
<title>SAF-SCRUM</title>

<link rel='stylesheet' href='css/w3.css'>
<link rel='stylesheet' href='css/hideInput.css'>

<script type="text/javascript">
        function changeOnglet (name) {
                document.getElementById('onglet_' + name).className += ' w3-green'
        }
</script>

<div class="w3-container w3-teal">
        <div class="w3-left-align w3-padding"><h2><%= projectName %></h2></div>
        <div class="w3-display-topmiddle"><a href="/projects" class="w3-button w3-teal w3-hover-green"><h2>Project List</h2></a></div>
        <div class="w3-display-topright"><a href="/logout" class="w3-button w3-teal w3-hover-green"><h2>logout</h2></a></div>
   </div>
   <div class="w3-bar w3-dark-grey">
        <a href="\summary" class="w3-bar-item w3-button w3-mobile" id="onglet_summary" style="width:12.5%" onclick="changeOnglet('summary')">Summary</a>
        <a href="\issues" class="w3-bar-item w3-button w3-mobile" id="onglet_issues" style="width:12.5%" onclick="changeOnglet('issues')">Issues</a>
        <a href="\tasks" class="w3-bar-item w3-button w3-mobile" id="onglet_tasks" style="width:12.5%" onclick="changeOnglet('tasks')">Tasks</a>
        <a href="\tests" class="w3-bar-item w3-button w3-mobile" id="onglet_tests" style="width:12.5%" onclick="changeOnglet('tests')">Tests</a>
        <a href="\sprints" class="w3-bar-item w3-button w3-mobile" id="onglet_sprints" style="width:12.5%" onclick="changeOnglet('sprints')">Sprints</a>
        <a href="\releases" class="w3-bar-item w3-button w3-mobile" id="onglet_releases" style="width:12.5%" onclick="changeOnglet('releases')">Releases</a>
        <a href="\documentation" class="w3-bar-item w3-button w3-mobile" id="onglet_documentation" style="width:12.5%" onclick="changeOnglet('documentation')">Documentation</a>
        <a href="\projectManagement" class="w3-bar-item w3-button w3-mobile" id="onglet_projectManagement" style="width:12.5%" onclick="changeOnglet('projectManagement')">Project Management</a>
    </div>
</div>